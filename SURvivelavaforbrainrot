-- [[ NX2 HUB - PRECISE EDITION ]] --

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local LocalPlayer = Players.LocalPlayer

-- [ ÿ≠ŸÅÿ∏ ÿßŸÑÿ≠ÿßŸÑÿßÿ™ ] --
getgenv().States = getgenv().States or {}

local Theme = {
    GalaxyBase = Color3.fromRGB(15, 10, 30),
    HeaderColor = Color3.fromRGB(40, 20, 80),
    ButtonOn = Color3.fromRGB(0, 255, 150),
    ButtonOff = Color3.fromRGB(45, 45, 60),
    VIP_Yellow = Color3.fromRGB(255, 215, 0),
    WhiteText = Color3.fromRGB(255, 255, 255),
    Black = Color3.fromRGB(0, 0, 0)
}

-- [ ÿ•ŸÜÿ¥ÿßÿ° ÿßŸÑŸàÿßÿ¨Ÿáÿ© ] --
local ScreenGui = Instance.new("ScreenGui", game:GetService("CoreGui"))
ScreenGui.Name = "NX2_ULTIMATE_CLEAN"

local OpenIcon = Instance.new("TextButton", ScreenGui)
OpenIcon.Size = UDim2.new(0, 60, 0, 60); OpenIcon.Position = UDim2.new(0.02, 0, 0.45, 0)
OpenIcon.BackgroundColor3 = Theme.Black; OpenIcon.Text = "NX2"; OpenIcon.TextColor3 = Theme.WhiteText
OpenIcon.Font = "GothamBlack"; OpenIcon.TextSize = 18; OpenIcon.Visible = false
Instance.new("UICorner", OpenIcon).CornerRadius = UDim.new(1, 0)

local MainFrame = Instance.new("Frame", ScreenGui)
MainFrame.Size = UDim2.new(0, 330, 0, 480); MainFrame.Position = UDim2.new(0.5, -165, 0.5, -240)
MainFrame.BackgroundColor3 = Theme.GalaxyBase; Instance.new("UICorner", MainFrame).CornerRadius = UDim.new(0, 15)

local Header = Instance.new("Frame", MainFrame)
Header.Size = UDim2.new(1, 0, 0, 80); Header.BackgroundColor3 = Theme.HeaderColor; Instance.new("UICorner", Header).CornerRadius = UDim.new(0, 15)

local Title = Instance.new("TextLabel", Header)
Title.Size = UDim2.new(1, 0, 1, 0); Title.Position = UDim2.new(0, 20, 0, 0)
Title.Text = "NX2 HUB"; Title.Font = "GothamBlack"; Title.TextSize = 24; Title.TextColor3 = Theme.WhiteText; Title.TextXAlignment = "Left"; Title.BackgroundTransparency = 1

local CloseBtn = Instance.new("TextButton", Header)
CloseBtn.Size = UDim2.new(0, 35, 0, 35); CloseBtn.Position = UDim2.new(1, -45, 0, 22)
CloseBtn.Text = "X"; CloseBtn.BackgroundColor3 = Theme.Black; CloseBtn.TextColor3 = Theme.WhiteText; Instance.new("UICorner", CloseBtn)

CloseBtn.MouseButton1Click:Connect(function() MainFrame.Visible = false; OpenIcon.Visible = true end)
OpenIcon.MouseButton1Click:Connect(function() OpenIcon.Visible = false; MainFrame.Visible = true end)

local Scroll = Instance.new("ScrollingFrame", MainFrame)
Scroll.Size = UDim2.new(1, -20, 1, -160); Scroll.Position = UDim2.new(0, 10, 0, 150); Scroll.BackgroundTransparency = 1; Scroll.ScrollBarThickness = 0
Instance.new("UIListLayout", Scroll).Padding = UDim.new(0, 8)

local function CreateButton(text, icon, key, color, callback)
    local btn = Instance.new("TextButton", Scroll)
    btn.Size = UDim2.new(1, 0, 0, 50); btn.BackgroundColor3 = Theme.ButtonOff; btn.Text = ""
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 10)
    local lbl = Instance.new("TextLabel", btn)
    lbl.Size = UDim2.new(1, 0, 1, 0); lbl.Font = "GothamBold"; lbl.TextSize = 14; lbl.TextColor3 = Theme.WhiteText; lbl.BackgroundTransparency = 1
    local function update()
        btn.BackgroundColor3 = getgenv().States[key] and (color or Theme.ButtonOn) or Theme.ButtonOff
        lbl.Text = icon .. "  " .. text .. (getgenv().States[key] and ": ON" or ": OFF")
        lbl.TextColor3 = getgenv().States[key] and Theme.Black or Theme.WhiteText
    end
    update()
    btn.MouseButton1Click:Connect(function() getgenv().States[key] = not getgenv().States[key]; update(); callback(getgenv().States[key]) end)
end

-- [[ ÿ™ÿßÿ®ÿ© Main ]] --
local function LoadMain()
    for _, v in pairs(Scroll:GetChildren()) do if v:IsA("TextButton") then v:Destroy() end end
    
    -- God Mode (ŸäŸÇÿ™ŸÑ ÿπŸÜÿØ ÿßŸÑÿ•ÿ∑ŸÅÿßÿ°)
    CreateButton("God Mode", "üõ°Ô∏è", "God", nil, function(s) 
        task.spawn(function()
            while task.wait(0.3) do
                if not getgenv().States["God"] then
                    -- ÿ•ÿ±ÿ¨ÿßÿπ ÿßŸÑŸÑŸÖÿ≥ ŸÑŸÑÿßŸÅÿß ÿπŸÜÿØ ÿßŸÑÿ•ÿ∑ŸÅÿßÿ°
                    for _, v in pairs(workspace:GetDescendants()) do
                        if v:IsA("BasePart") and (v.Name:lower():find("lava") or v.Name:lower():find("kill")) then v.CanTouch = true end
                    end
                    break
                end
                pcall(function()
                    LocalPlayer.Character.Humanoid.Health = 100
                    for _, v in pairs(workspace:GetDescendants()) do
                        if v:IsA("BasePart") and (v.Name:lower():find("lava") or v.Name:lower():find("kill")) then v.CanTouch = false end
                    end
                end)
            end
        end)
    end)

    CreateButton("Infinite Jump", "ü¶ò", "InfJ", nil, function(s) 
        local c; c = UserInputService.JumpRequest:Connect(function() if getgenv().States["InfJ"] then LocalPlayer.Character.Humanoid:ChangeState(3) end end)
    end)
    
    CreateButton("Anti-AFK", "üí§", "AFK", nil, function(s)
        LocalPlayer.Idled:Connect(function() if getgenv().States["AFK"] then game:GetService("VirtualUser"):CaptureController(); game:GetService("VirtualUser"):ClickButton2(Vector2.new()) end end)
    end)

    CreateButton("FPS Boost", "‚ö°", "FPS", nil, function(s) 
        if s then settings().Rendering.QualityLevel = 1; game:GetService("Lighting").GlobalShadows = false else game:GetService("Lighting").GlobalShadows = true end 
    end)

    CreateButton("Unlock Zoom", "üîì", "Zoom", nil, function(s) LocalPlayer.CameraMaxZoomDistance = s and 99999 or 128 end)
end

-- [[ ÿ™ÿßÿ®ÿ© Auto ]] --
local function LoadAuto()
    for _, v in pairs(Scroll:GetChildren()) do if v:IsA("TextButton") then v:Destroy() end end
    
    -- OPEN VIP (ŸäÿÆŸÅŸä ŸÉŸÑŸÖÿ© ÿßŸÑÿ¥ÿ±ÿßÿ° ŸÅŸàÿ±ÿßŸã)
    CreateButton("OPEN VIP", "üëë", "VipPass", Theme.VIP_Yellow, function(s)
        task.spawn(function()
            while getgenv().States["VipPass"] do
                pcall(function()
                    for _, v in pairs(workspace:GetDescendants()) do
                        if v:IsA("BasePart") and (v.Name:lower():find("vip") or v.Name:lower():find("premium")) then
                            v.CanCollide = false; v.Transparency = 0.5
                        end
                    end
                    -- ÿ•ÿÆŸÅÿßÿ° ŸÉŸÑŸÖÿ© ÿßŸÑÿ¥ÿ±ÿßÿ° ÿ™ŸÖÿßŸÖÿßŸã
                    local purchaseUI = game:GetService("CoreGui"):FindFirstChild("PurchasePrompt")
                    if purchaseUI then purchaseUI.Enabled = false end
                end)
                task.wait(0.1) -- ŸÅÿ≠ÿµ ÿ≥ÿ±Ÿäÿπ ÿ¨ÿØÿßŸã ŸÑŸÑÿ≠ÿ∞ŸÅ
            end
            -- ÿ•ÿ±ÿ¨ÿßÿπ ŸÉŸÑ ÿ¥Ÿäÿ° ÿπŸÜÿØ ÿßŸÑÿ•ÿ∑ŸÅÿßÿ°
            local purchaseUI = game:GetService("CoreGui"):FindFirstChild("PurchasePrompt")
            if purchaseUI then purchaseUI.Enabled = true end
            for _, v in pairs(workspace:GetDescendants()) do
                if v:IsA("BasePart") and (v.Name:lower():find("vip") or v.Name:lower():find("premium")) then v.CanCollide = true; v.Transparency = 0 end
            end
        end)
    end)
    
    CreateButton("Auto Rebirth", "üîÑ", "Rebirth", nil, function(s)
        task.spawn(function()
            while getgenv().States["Rebirth"] do
                pcall(function()
                    for _, v in pairs(workspace:GetDescendants()) do
                        if v:IsA("ProximityPrompt") and v.ObjectText:lower():find("rebirth") then
                            v.HoldDuration = 0; fireproximityprompt(v)
                        end
                    end
                end)
                task.wait(1)
            end
        end)
    end)
end

-- [[ ÿ™ÿßÿ®ÿ© ‚òÖ ]] --
local function LoadStar()
    for _, v in pairs(Scroll:GetChildren()) do if v:IsA("TextButton") then v:Destroy() end end
    local HopBtn = Instance.new("TextButton", Scroll)
    HopBtn.Size = UDim2.new(1, 0, 0, 100); HopBtn.BackgroundColor3 = Theme.HeaderColor; HopBtn.Text = "üöÄ SERVER HOP"; HopBtn.TextColor3 = Theme.WhiteText; HopBtn.Font = "GothamBlack"; HopBtn.TextSize = 25; Instance.new("UICorner", HopBtn).CornerRadius = UDim.new(0, 15)
    HopBtn.MouseButton1Click:Connect(function()
        local s = game:GetService("HttpService"):JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100"))
        for _, v in pairs(s.data) do if v.playing < v.maxPlayers and v.id ~= game.JobId then game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, v.id, LocalPlayer) break end end
    end)
end

-- [ ÿßŸÑÿ™ÿ®ÿØŸäŸÑ ŸàÿßŸÑÿ≥ÿ≠ÿ® ] --
local TabHolder = Instance.new("Frame", MainFrame)
TabHolder.Size = UDim2.new(1, -20, 0, 45); TabHolder.Position = UDim2.new(0, 10, 0, 95); TabHolder.BackgroundTransparency = 1
local function AddTab(n, p, f)
    local b = Instance.new("TextButton", TabHolder); b.Size = UDim2.new(0.31, 0, 1, 0); b.Position = p
    b.Text = n; b.BackgroundColor3 = Theme.HeaderColor; b.TextColor3 = Theme.WhiteText; b.Font = "GothamBold"; Instance.new("UICorner", b)
    b.MouseButton1Click:Connect(f)
end
AddTab("Main", UDim2.new(0, 0, 0, 0), LoadMain)
AddTab("Auto", UDim2.new(0.34, 0, 0, 0), LoadAuto)
AddTab("‚òÖ", UDim2.new(0.68, 0, 0, 0), LoadStar)

LoadMain()
local function Drag(o)
    local d, i, s, p
    o.InputBegan:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then d = true; s = input.Position; p = o.Position end end)
    o.InputChanged:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then i = input end end)
    RunService.RenderStepped:Connect(function() if d and i then local delta = i.Position - s; o.Position = UDim2.new(p.X.Scale, p.X.Offset + delta.X, p.Y.Scale, p.Y.Offset + delta.Y) end end)
    UserInputService.InputEnded:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then d = false end end)
end
Drag(MainFrame); Drag(OpenIcon)